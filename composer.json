{
    "name": "phpactor/worse-reflection-extension",
    "description": "Worse Reflection",
    "license": "MIT",
    "type": "phpactor-extension",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "/home/daniel/www/phpactor/class-to-file-extension"
        }
    ],
    "require": {
        "phpactor/container": "^1.2",
        "phpactor/worse-reflection": "dev-master",
        "phpactor/logging-extension": "^0.1",
        "phpactor/file-path-resolver-extension": "dev-master",
        "phpactor/class-to-file-extension": "dev-master",
        "phpactor/composer-autoloader-extension": "dev-master"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.5",
        "phpunit/phpunit": "^7.4",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\WorseReflection\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\WorseReflection\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\Exension\\WorseReflectionExtension"
    }
}
