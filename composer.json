{
    "name": "phpactor/behat-extension",
    "description": "Behat Support",
    "license": "MIT",
    "type": "phpactor-extension",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],

    "require": {
        "phpactor/container": "^1.0",
        "phpactor/completion": "~0.1",
        "phpactor/file-path-resolver-extension": "~0.1",
        "phpactor/worse-reflection-extension": "~0.1",
        "behat/gherkin": "^4.5",
        "phpactor/completion-extension": "~0.1",
        "symfony/yaml": "^4.0|^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.5",
        "phpunit/phpunit": "^7.4",
        "friendsofphp/php-cs-fixer": "^2.13",
        "behat/behat": "^3.5",
        "phpactor/test-utils": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\Behat\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\Behat\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\Extension\\Behat\\BehatExtension"
    }
}
