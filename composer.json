{
    "name": "phpactor/worse-language-server",
    "description": "Wose Reflection completion, definition, etc",
    "license": "MIT",
    "type": "phpactor-extension",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],

    "require": {
        "phpactor/container": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.7",
        "phpunit/phpunit": "^7.5",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\WorseLanguageServer\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\WorseLanguageServer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\Extension\\WorseLanguageServer\\WorseLanguageServerExtension",
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
