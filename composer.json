{
    "name": "phpactor/phpactor-packages",
    "description": "Monorepo for Phpactor Packages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "thecodingmachine/safe": "^0.1.16",
        "phpunit/phpunit": "~7.0",
        "friendsofphp/php-cs-fixer": "~2.15.0",
        "phpstan/phpstan": "~0.11.0",
        "phpbench/phpbench": "dev-master",
        "symfony/yaml": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\ClassMover\\": "library/class-mover/lib/",
            "Phpactor\\ClassFileConverter\\": [
                "library/class-to-file/lib/",
                "library/path-finder/lib/"
            ],
            "Phpactor\\CodeBuilder\\": "library/code-builder/lib/",
            "Phpactor\\CodeTransform\\": "library/code-transform/lib/",
            "Phpactor\\Completion\\": "library/completion/lib/",
            "Phpactor\\ConfigLoader\\": "library/config-loader/lib/",
            "Phpactor\\Container\\": "library/container/lib/",
            "Phpactor\\Docblock\\": "library/docblock/lib/",
            "Phpactor\\FilePathResolver\\": "library/file-path-resolver/lib/",
            "Phpactor\\MapResolver\\": "library/map-resolver/lib/",
            "Phpactor\\ReferenceFinder\\": "library/reference-finder/lib/",
            "Phpactor\\Filesystem\\": "library/source-code-filesystem/lib/",
            "Phpactor\\TestUtils\\": "library/test-utils/lib/",
            "Phpactor\\TextDocument\\": "library/text-document/lib/",
            "Phpactor\\WorseReferenceFinder\\": "library/worse-reference-finders/lib/",
            "Phpactor\\WorseReflection\\": "library/worse-reflection/lib/",
            "Phpactor\\Extension\\ClassToFile\\": "extensions/class-to-file-extension/lib/",
            "Phpactor\\Extension\\CodeTransform\\": "extensions/code-transform-extension/lib/",
            "Phpactor\\Extension\\Completion\\": "extensions/completion-extension/lib/",
            "Phpactor\\Extension\\CompletionRpc\\": "extensions/completion-rpc-extension/lib/",
            "Phpactor\\Extension\\CompletionWorse\\": "extensions/completion-worse-extension/lib/",
            "Phpactor\\Extension\\ComposerAutoloader\\": "extensions/composer-autoloader-extension/lib/",
            "Phpactor\\Extension\\Console\\": "extensions/console-extension/lib/",
            "Phpactor\\Extension\\ExtensionManager\\": "extensions/extension-manager-extension/lib/",
            "Phpactor\\FilePathResolverExtension\\": "extensions/file-path-resolver-extension/lib/",
            "Phpactor\\Extension\\Logger\\": "extensions/logging-extension/lib/",
            "Phpactor\\Extension\\ReferenceFinder\\": "extensions/reference-finder-extension/lib/",
            "Phpactor\\Extension\\ReferenceFinderRpc\\": "extensions/reference-finder-rpc-extension/lib/",
            "Phpactor\\Extension\\Rpc\\": "extensions/rpc-extension/lib/",
            "Phpactor\\Extension\\SourceCodeFilesystem\\": "extensions/source-code-filesystem-extension/lib/",
            "Phpactor\\Extension\\WorseReferenceFinder\\": "extensions/worse-reference-finder-extension/lib/",
            "Phpactor\\Extension\\WorseReflection\\": "extensions/worse-reflection-extension/lib/"
        }
    },
    "replace": {
        "phpactor/class-mover": "dev-master",
        "phpactor/class-to-file": "0.2.0",
        "phpactor/code-builder": "0.2.0",
        "phpactor/code-transform": "0.1.1",
        "phpactor/completion": "0.2.1",
        "phpactor/config-loader": "0.1.0",
        "phpactor/container": "1.3.0",
        "phpactor/docblock": "0.3.0",
        "phpactor/file-path-resolver": "0.8.0",
        "phpactor/map-resolver": "1.2.0",
        "phpactor/path-finder": "dev-master",
        "phpactor/reference-finder": "0.1.0",
        "phpactor/source-code-filesystem": "0.1.0",
        "phpactor/test-utils": "1.0.0",
        "phpactor/text-document": "1.0.0",
        "phpactor/worse-reference-finders": "0.1.0",
        "phpactor/worse-reflection": "0.3.0",
        "phpactor/class-to-file-extension": "0.1.0",
        "phpactor/code-transform-extension": "dev-master",
        "phpactor/completion-extension": "0.1.0",
        "phpactor/completion-rpc-extension": "0.1.0",
        "phpactor/completion-worse-extension": "0.1.1",
        "phpactor/composer-autoloader-extension": "dev-master",
        "phpactor/console-extension": "0.1.1",
        "phpactor/extension-manager-extension": "0.8.0",
        "phpactor/file-path-resolver-extension": "0.3.0",
        "phpactor/logging-extension": "0.3.1",
        "phpactor/reference-finder-extension": "0.1.1",
        "phpactor/reference-finder-rpc-extension": "0.1.1",
        "phpactor/rpc-extension": "0.1.1",
        "phpactor/source-code-filesystem-extension": "0.1.2",
        "phpactor/worse-reference-finder-extension": "dev-master",
        "phpactor/worse-reflection-extension": "0.2.1"
    },
    "require": {
        "php": "^7.1",
        "symfony/filesystem": "^2.7|^3.0",
        "microsoft/tolerant-php-parser": "~0.0.1",
        "psr/log": "~1.0",
        "webmozart/path-util": "~2.3",
        "twig/twig": "^2.4",
        "dnoegel/php-xdg-base-dir": "^0.1.0",
        "psr/container": "^1.0",
        "jetbrains/phpstorm-stubs": "*",
        "symfony/console": "^4.1",
        "composer/composer": "^1.7",
        "monolog/monolog": "^1.23"
    },
    "scripts": {
        "test": "php scripts/test.php",
        "build": "php scripts/build.php"
    }
}
