{
    "name": "phpactor/source-code-filesystem-extension",
    "description": "Source Code Filesystem Extension",
    "license": "MIT",
    "type": "phpactor-extension",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],

    "require": {
        "phpactor/container": "^1.2",
        "phpactor/source-code-filesystem": "dev-master",
        "phpactor/file-path-resolver-extension": "dev-master",
        "phpactor/composer-autoloader-extension": "dev-master"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.5",
        "phpunit/phpunit": "^7.4",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\SourceCodeFilesystem\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\SourceCodeFilesystem\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\Extension\\SourceCodeFilesystem\\SourceCodeFilesystemExtension"
    }
}
