            "Phpactor\\FilePathResolverExtension\\Tests\\": "extension/file-path-resolver-extension/tests/",
            "Phpactor\\Extension\\Logger\\Tests\\": "extension/logging-extension/tests/",
            "Phpactor\\Extension\\ReferenceFinder\\Tests\\": "extension/reference-finder-extension/tests/",
            "Phpactor\\Extension\\ReferenceFinderRpc\\Tests\\": "extension/reference-finder-rpc-extension/tests/",
            "Phpactor\\Extension\\Rpc\\Tests\\": "extension/rpc-extension/tests/",
            "Phpactor\\Extension\\SourceCodeFilesystem\\Tests\\": "extension/source-code-filesystem-extension/tests/",
            "Phpactor\\Extension\\WorseReferenceFinder\\Tests\\": "extension/worse-reference-finder-extension/tests/",
