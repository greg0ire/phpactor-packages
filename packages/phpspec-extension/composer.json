{
    "name": "phpactor/phpspec-extension",
    "description": "Integration for Phpspec",
    "license": "MIT",
    "type": "phpactor-extension",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],

    "require": {
        "phpactor/container": "^1.0",
        "phpactor/worse-reflection-extension": "~0.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.6",
        "phpunit/phpunit": "^7.5",
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpspec/phpspec": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\PhpSpec\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\PhpSpec\\Tests\\": "tests/",
            "spec\\Phpactor\\Extension\\PhpSpec\\Tests\\": "spec/"
        }
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\Extension\\PhpSpec\\PhpSpecExtension",
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
